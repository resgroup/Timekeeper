<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- Disable the online NuGet repository to force the build server to always use local packages -->
  <disabledPackageSources>
    <add key="nuget.org" value="true" />
    <add key="Microsoft and .NET" value="true" />
  </disabledPackageSources>
  <activePackageSource>
    <add key="All" value="(Aggregate source)" />
  </activePackageSource>
</configuration>